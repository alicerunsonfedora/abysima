{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".obsidian/": true,
        "__pycache__": true,
        ".ipynb_checkpoints": true
    },
    "jupyter.jupyterServerType": "local",
    "workbench.colorTheme": "Default Light+",
    "jupyter.themeMatplotlibPlots": true,
    "editor.fontFamily": "'Cascadia Code', 'JetBrains Mono', 'SF Mono', SFMonoRegular, -system-monospaced, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.lineHeight": 25
}